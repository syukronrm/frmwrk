<div class="container">
  <%= form_for @changeset, auth_path(@conn, :create_login), fn f -> %>
    <label>
      Email: <%= text_input f, :email %>
    </label>

    <label>
      Password: <%= password_input f, :password %>
    </label>

    <%= submit "Submit" %>
  <% end %>
  <a class="btn btn-outline-primary" href="/auth/google">Masuk dengan Google</a>
</div>
